<?php

/**
 * @file
 * Page Load Progress allows you to set a screen lock showing a spinner when
 * the user clicks on an element that triggers a time consuming task.
 */

/**
 * Implements hook_permission().
 */
function page_load_progress_permission() {
  return [
    'administer Page Load Progress settings' => [
      'title' => t('Administer Page Load Progress settings'),
    ],
  ];
}
